<section id="connect" class="section-padding contact-section">
  <div class="container">
    <div class="row">
      <div class="col-12 text-center">
        <nav>
          <div class="nav nav-pills justify-content-center homepage-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="quote-tab" data-toggle="tab" href="#quote" role="tab"
               aria-controls="nav-quote"
               aria-selected="true">QUOTE</a>
          </div>
        </nav>
      </div>
    </div>
    <div class="tab-content">
      <div class="tab-pane fade show active" id="quote" role="tabpanel" aria-labelledby="quote-tab">
        <div class="row">
          <div class="col-md-6 col-lg-5 align-self-center">
            <h3>Request a Quote</h3>
            <p>Whether its a small or large project, we would love to hear more about your application and how we can
              help provide a solution. Feel
              free to get in touch with us.</p>
            <span class="hr-image">
              <img alt="Image"
                   src="<%= storageHost %>/images/hr-line.svg"
                   width="200" height="14">
            </span>
          </div>
          <div class="col-md-6 col-lg-6 offset-lg-1">
            <form>
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Name">
              </div>
              <div class="form-group">
                <input type="email" class="form-control" placeholder="Email">
              </div>
              <div class="form-group">
                <textarea class="form-control" rows="5" placeholder="Message"></textarea>
              </div>
              <div class="form-group">
                <input type="submit" class="btn btn-theme-alt sm-btn text-uppercase" value="Send Message"/>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

